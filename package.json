{
  "name": "new",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.5",
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "clsx": "^1.0.4",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "http": "0.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "node-mailjet": "^3.3.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-particles-js": "^2.7.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-select": "^3.0.8",
    "util": "^0.12.1"
  },
  "scripts": {
    "start:server": "nodemon api/_server.js",
    "start:client": "react-scripts start",
    "all": "concurrently --kill-others \"nodemon api/_server.js\" \"react-scripts start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:4000",
  "devDependencies": {
    "concurrently": "^5.0.0"
  },
  "now": {
    "alias": "notez",
    "name": "notez",
    "env": {
      "PORT": "4000",
      "SECRET": "@secret_notez",
      "MONGODB_URI": "@mongodb_uri_notez",
      "SERVICE": "@service_notez",
      "MJ_APIKEY_PUBLIC": "@mj_apikey_public",
      "MJ_APIKEY_PRIVATE": "@mj_apikey_private"
    },
    "routes": [
      {
        "src": "/",
        "dest": "index.html"
      },
      {
        "src": "/api/games/(.+)",
        "dest": "/api/games.js"
      },
      {
        "src": "/api/users/(.+)",
        "dest": "/api/users.js"
      },
      {
        "src": "/api/characters/(.+)",
        "dest": "/api/characters.js"
      },
      {
        "src": "/api/filters/(.+)",
        "dest": "/api/filters.js"
      },
      {
        "src": "/api/gameNotes/(.+)",
        "dest": "/api/gameNotes.js"
      },
      {
        "src": "/api/invites/(.+)",
        "dest": "/api/invites.js"
      },
      {
        "src": "/api/playerNotes/(.+)",
        "dest": "/api/playerNotes.js"
      }
    ]
  }
}
